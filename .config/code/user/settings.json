{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "breadcrumbs.enabled": false,

  "editor.bracketPairColorization.enabled": false,
  "editor.fontFamily": "Cascadia Mono",
  "editor.fontSize": 15,
  "editor.fontWeight": "350",
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "off",
  "editor.lineHeight": 22,
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.stickyScroll.enabled": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["keyword.other.create.sql", "keyword.other.DML.II.sql"],
        "settings": {
          "foreground": "#50a2e5"
        }
      }
    ]
  },

  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,

  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,

  "git.openRepositoryInParentFolders": "never",

  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.lens.enable": false,

  "security.workspace.trust.enabled": false,

  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeight": "350",

  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "window.title": "${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "window.titleBarStyle": "custom",
  "window.titleSeparator": " â™¡ ",

  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#50a2e5",
    "activityBar.background": "#000000",
    "editor.background": "#000000",
    "editor.findMatchBackground": "#3c3c3c",
    "editor.findMatchBorder": "#50a2e5aa",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGutter.background": "#000000",
    "editorHoverWidget.background": "#000000",
    "editorHoverWidget.border": "#50a2e5",
    "editorWidget.background": "#000000",
    "focusBorder": "#000000",
    "keybindingLabel.background": "#272727",
    "keybindingLabel.border": "#272727",
    "keybindingLabel.bottomBorder": "#272727",
    "list.activeSelectionBackground": "#3c3c3c",
    "list.focusAndSelectionOutline": "#50a2e5",
    "list.hoverBackground": "#3c3c3c",
    "list.inactiveSelectionBackground": "#3c3c3c",
    "menu.background": "#000000",
    "menu.border": "#50a2e5",
    "menu.separatorBackground": "#50a2e5",
    "panel.background": "#000000",
    "panel.border": "#000000",
    "quickInput.background": "#000000",
    "quickInputList.focusBackground": "#3c3c3c",
    "quickInputList.focusIconForeground": "#e6e6e6",
    "sideBar.background": "#000000",
    "sideBar.border": "#000000",
    "sideBarSectionHeader.background": "#000000",
    "sideBarTitle.foreground": "#bababa",
    "statusBar.background": "#000000",
    "statusBar.border": "#000000",
    "statusBar.focusBorder": "#000000",
    "tab.activeBackground": "#000000",
    "tab.activeBorderTop": "#50a2e5",
    "tab.inactiveBackground": "#000000",
    "terminal.background": "#000000",
    "titleBar.activeBackground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "widget.border": "#50a2e5"
  },
  "workbench.colorTheme": "dPlanets Contrast Neptune",
  "workbench.iconTheme": "quill-icons-minimal",
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "Tabler",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "none"
}
