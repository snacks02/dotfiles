{
  "auto_install_extensions": {
    "fish": true,
    "html": true,
    "one-dark-darkened": true,
    "ruby": true,
    "toml": true
  },
  "buffer_font_family": "Geist Mono",
  "buffer_font_features": {
    "liga": 0
  },
  "buffer_font_size": 15,
  "buffer_line_height": {
    "custom": 1.45
  },
  "collaboration_panel": {
    "button": false
  },
  "debugger": {
    "button": false
  },
  "diagnostics": {
    "button": false
  },
  "format_on_save": "off",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "git_panel": {
    "button": false
  },
  "indent_guides": {
    "enabled": false
  },
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "project_panel": {
    "auto_fold_dirs": false,
    "default_width": 260,
    "indent_guides": {
      "show": "never"
    }
  },
  "show_whitespaces": "none",
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "button": false
  },
  "theme": {
    "dark": "One Dark - Darkened",
    "light": "One Dark - Darkened"
  },
  "title_bar": {
    "show_sign_in": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "ui_font_family": "Inter Display"
}
